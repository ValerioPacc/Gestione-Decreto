<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<SelectDialog
		title="{i18n>HelpContratto}"
		growing="true"
		growingThreshold="9999"
		class="sapUiPopupWithPadding"
		items="{comboBox>/Beneficiario}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpCloseBen"
		cancel="_handleValueHelpClose"
        app:filterTableField="beneficiario1"
		>
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{comboBox>nome} {comboBox>cognome}"
			app:key="{comboBox>id}"
			 />
	</SelectDialog>	
</core:FragmentDefinition>